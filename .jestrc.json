{
    "automock": true,
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": ["/node_modules/", "/mocks/"],
    "testRegex": "\\.test\\.js$",
    "roots": ["<rootDir>/tests"],
    "unmockedModulePathPatterns": [
        "mocks",
        "<rootDir>/node_modules/jest-cli",
        "<rootDir>/node_modules/jasmine-expect"
    ]
}
