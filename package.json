{
    "name": "jest-ex",
    "description": "A runner and a transformer to simplify (a little bit) your work with Jest.",
    "version": "5.0.0",
    "repository": "homer0/jest-ex",
    "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
    "license": "MIT",
    "keywords": [
      "jest",
      "jasmine",
      "tests",
      "unit"
    ],
    "dependencies": {
      "glob": "7.1.3",
      "@babel/core": "7.2.2",
      "@babel/plugin-transform-runtime": "7.2.0",
      "@babel/preset-env": "^7.3.1",
      "yargs": "12.0.5",
      "html-loader": "0.5.5",
      "jest-cli": "^24.0.0"
    },
    "devDependencies": {
      "@babel/cli": "7.2.3",
      "babel-jest": "24.0.0",
      "eslint": "5.13.0",
      "eslint-config-airbnb-base": "13.1.0",
      "eslint-plugin-import": "2.16.0",
      "eslint-plugin-node": "8.0.1",
      "jasmine-expect": "4.0.1",
      "esdoc": "1.1.0",
      "esdoc-standard-plugin": "1.0.0",
      "coveralls": "3.0.2"
    },
    "engine-strict": true,
    "engines": {
      "node": ">=8.0.0",
      "npm": ">=3.0.0"
    },
    "main": "dist/index.js",
    "scripts": {
      "hooks": "./utils/hooks/install",
      "build": "./utils/scripts/build",
      "test": "./utils/scripts/test",
      "lint": "./utils/scripts/lint",
      "docs": "./utils/scripts/docs",
      "prepublish": "npm run build"
    }
}
