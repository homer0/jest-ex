{
    "name": "jest-ex",
    "description": "A runner and a transformer to simplify (a little bit) your work with Jest.",
    "version": "2.0.0",
    "repository": "homer0/jest-ex",
    "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
    "license": "MIT",
    "keywords": [
        "jest",
        "jasmine",
        "tests",
        "unit"
    ],
    "dependencies": {
        "glob": "7.1.2",
        "babel-core": "6.25.0",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-es2015-node6": "0.4.0",
        "yargs": "8.0.1",
        "jest-cli": "20.0.4",
        "html-loader": "0.4.5"
    },
    "devDependencies": {
        "babel-cli": "6.24.1",

        "eslint": "3.19.0",
        "eslint-config-airbnb": "15.0.1",

        "eslint-plugin-jsx-a11y": "5.0.3",
        "eslint-plugin-import": "2.3.0",
        "eslint-plugin-react": "7.0.1",

        "jasmine-expect": "3.7.0",

        "esdoc": "0.4.8",
        "esdoc-es7-plugin": "0.0.3",
        "esdoc-uploader": "1.0.1",

        "coveralls": "2.13.1"
    },
    "engine-strict": true,
    "engines": {
        "node": ">=6.10.0",
        "npm": ">=3.0.0"
    },
    "main": "dist/index.js",
    "scripts": {
        "install-hooks": "./utils/hooks/install",
        "build": "./utils/scripts/build",
        "test": "./utils/scripts/test",
        "lint": "./utils/scripts/lint",
        "docs": "./utils/scripts/docs",
        "docs:deploy": "./utils/scripts/deploy-docs",
        "prepublish": "npm run build"
    }
}
